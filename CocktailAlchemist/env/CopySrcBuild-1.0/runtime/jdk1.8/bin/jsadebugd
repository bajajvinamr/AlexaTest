#!/apollo/sbin/envroot /bin/bash
#
# Generated by Brazil CLI 2.0.208096.0 at 2023-09-10 20:52:04 +0530
#

if [ -z "$BRAZIL_JAVA18_HOME" ]
then
    cat >&2 << EOF
ERROR: You have no preference setting for Java 8.
Run brazil setup --java
EOF
    exit 1
elif [ ! -d "$BRAZIL_JAVA18_HOME" ]
then
    cat >&2 << EOF
ERROR: Your Java 8 preference is not a directory: $BRAZIL_JAVA18_HOME
Run brazil setup --java
EOF
    exit 1
fi
exec -a "$0" "$BRAZIL_JAVA18_HOME/bin/jsadebugd" "$@"
